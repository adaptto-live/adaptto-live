    "eslint": "^8.43.0",
    "eslint-plugin-vue": "^9.15.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "typescript": "~5.1.3",
